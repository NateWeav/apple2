    </div>
    <div class="content clearfix">
        <p><strong>${translate.overlay.error.description}</strong></p>
        <div class="error-actions">
            <button class="cancel continue"><span class="replaced">${translate.store.button_text.continue}</span></button>
        </div>
    </div>
    <div class="footer empty-footer">
        <p>&nbsp;</p>
    </div>
</div>
</script>

        <script type="text/mustache" id="{{OverlayComponent}}">
<materializer data-uid='{{dataUid}}-mat'
                data-shown='{materializerShown}'
                data-show-done='{dataShowDone}'
                data-hide-done='{dataHideDone}'
                class='ase-overlay-materializer{{#if dataSheet}} ase-overlay-sheetmaterializer ase-overlay-materializersheet{{/if}}'>
    {{#if dataHasScreen}}
        <computed-height data-type="document" data-recompute="{recomputeScreen}"
                        class="ase-overlay-screen" data-property="min-height"></computed-height>
    {{/if}}
    <div class="ase-overlay {{#if dataSheet}}ase-overlay-sheet{{else}}ase-overlay-popup{{/if}}"
         {{#if dataAriaLabelledby}}aria-labelledby={{dataAriaLabelledby}}{{/if}}
                 role="dialog" tabindex="-1">
        <content />
    </div>
</materializer>

</script>
	
	</body>
</html>