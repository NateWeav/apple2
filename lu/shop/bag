            }
            if (ametrics.getPageLoadTime) {
                s.eVar9 = ametrics.getPageLoadTime();
            }

            

            ametrics.mark();

            
            var s_code = s.t();if(s_code)document.write(s_code);

</script>


	  
 	</div>



	


	


</body>

</html>
