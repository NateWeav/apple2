    </script>
    
    <script>
        (function (window, $, as, can, undefined) {
            as.isDesktop = true;
            can.Mustache.registerHelper("isDesktop", function(options) {
                return as.isDesktop ? options.fn(options.contexts) : false;
            });
            can.Mustache.registerHelper("isMobile", function(options) {
                return !as.isDesktop ? options.fn(options.contexts) : false;
            });
            /* for gifting materializer */
            can.stache.registerHelper("editContentShown", function(options) {
                var content = options.fn(options.contexts),
                    isTrue = (content === 'true'),
                    isFalse = (content === 'false'),
                    isBool = isTrue || isFalse,
                    editContent_visible = options.context['modeTab'] === 'edit';
                return isBool ? (isTrue === editContent_visible) : (editContent_visible ? content : false);
            });
            /*for finance message(general) and installment message(total)*/
            can.stache.registerHelper("showFinancingMessage", function(type, options) {
                    var totalPattern = new RegExp("installments"),
                        /*options.context can access the data in the scope*/
                        content = options.context.financingMessage,
                        findTotalPattern = totalPattern.test(content);
                    if (findTotalPattern) {
                        if (type !== 'total') content = '';
                    } else {
                        if (type !== 'general') content = '';
                    }
                    return options.fn({
                     content: content
                    });
            });
            /*To show the Pay with Apple Pay button only if the user has a valid credit card in the Wallet*/
            can.stache.registerHelper("showApplePayButton", function(options) {
					var merchantIdentifier = options.context.merchantIdentifier;
					if(merchantIdentifier && window.ApplePaySession) {
						window.acStoreApplePay.canUseApplePay(merchantIdentifier).then(function(result) {
							!!result
								? document.documentElement.classList.add('supports-apw-activecard')
								: document.documentElement.classList.remove("supports-applepay", "supports-apw");
						}).catch(function(error) {});
					}
            });
            /*for NPI overlay focus */
            can.view.attr("data-focus-when", function( el, attrData ) {
                var handler = function(ev, newVal, oldVal){
                    if (newVal) {
                        setTimeout(function() { el.focus(); }, 1000);
                    }
                };

                var attrValues = el.getAttribute("data-focus-when").split(',');
                var itemsFocusInCompute = [];
                for(var i=0; i<attrValues.length; i+=1){
                    var focusInCompute = attrData.scope.compute(attrValues[i]);
                    focusInCompute.bind("change", handler);
                    itemsFocusInCompute.push(focusInCompute);
                }

                $(el).bind("remove", function(){
                    for(var i=0; i<itemsFocusInCompute.length; i+=1){
                        itemsFocusInCompute[i].unbind(handler);
                    }
                });
            });
            window.ctrl = new as.OrbitParser();
            window.ctrl.processNewData(JSON.parse($("#init_data").html()));
        }(window, jQuery, as, can));
    </script>
    
