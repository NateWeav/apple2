    <script type="text/mustache" data-render>
        <overlay data-uid="bag-installment-overlay" data-sheet="{{dataSheet}}" class="as-overlay{{#dataSheet}} as-overlay-fullscreen as-overlay-withscrim{{/dataSheet}}{{^dataSheet}} as-overlay-popup{{/dataSheet}} as-overlay-flex" data-shown="{bag-installment-overlay-loaded}" data-aria-labelledby="installment-overlay-header">
            <div class="as-overlay-content{{^dataSheet}} as-overlay-contentnopadding{{/dataSheet}}"> 
                <div class="as-installments">
                    <loader data-uid="bag-installment-overlay-loader" data-loaded="{bag-installment-overlay-loaded}"> </loader> 
                </div>
            </div> 
            <button class="as-overlay-close ase-overlay-close" data-ase-overlay="bag-installment-overlay" data-ase-click="hide"> 
                <i class="icon icon-close"></i> 
                <span class="a11y">
                    Fermer
                </span> 
            </button>  
