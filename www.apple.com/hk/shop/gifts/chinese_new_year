            window.onload = function() {
                if(window.as.AnimateOnScroll) {
                    window.as.AnimateOnScroll.init();
                }
            };
		</script>

        <script type="text/mustache" id="{{WebOverlay}}">
<div class="overlay" data-autom="overlay" tabindex="-1" {{#if isAriaDialog}}
	role=alertdialog aria-labelledby=overlayHeading{{uid}}
	{{/if}}>
	<span class="chrome tl"></span>
	<span class="chrome tr"></span>
	<span class="chrome top"></span>
	<span class="chrome left"></span>
	<span class="chrome right"></span>
	<span class="chrome bottom"></span>
	<span class="chrome bl"></span>
	<span class="chrome br"></span>
	<span class="chrome center"></span>
	<div class="container">
		<div class="content" id="overlayContent{{uid}}" style="{{{as.WebOverlay.contentSize width height}}}">
		{{#if contentIsHtml}}
			{{{content}}}
		{{else}}
			{{#if contentIsTemplate}}
				{{{as.include content}}}
			{{else}}
				{{content}}
			{{/if}}
		{{/if}}
		</div>
		{{#if recoveryOptions.length}}
		<ul class="buttons recoveryOptions">
			{{#recoveryOptions}}
			<li><button {{data 'recoveryOption'}}>{{text}}</button></li>
			{{/recoveryOptions}}
		</ul>
		{{/if}}
	</div>
	{{#if canClose}}<button class="as-overlay-close ase-overlay-close close" data-autom="overlayCloseButton"><i class="icon icon-close"></i><span class="a11y">{{{close}}}</span></button>{{/if}}
</div>

</script>
        
        <script type="text/mustache" id="{{WebOverlayError}}">
<div class="blue-header-popup">
    <div class="header">
        <h2>${translate.overlay.error.header}</h2>
    </div>
    <div class="content clearfix">
        <p><strong>${translate.overlay.error.description}</strong></p>
        <div class="error-actions">
            <button class="cancel continue"><span class="replaced">${translate.store.button_text.continue}</span></button>
        </div>
    </div>
    <div class="footer empty-footer">
        <p>&nbsp;</p>
    </div>
</div>
</script>

        <script type="text/mustache" id="{{OverlayComponent}}">
<materializer data-uid='{{dataUid}}-mat'
                data-shown='{materializerShown}'
                data-show-done='{dataShowDone}'
                data-hide-done='{dataHideDone}'
                class='ase-overlay-materializer{{#if dataSheet}} ase-overlay-sheetmaterializer ase-overlay-materializersheet{{/if}}'>
    {{#if dataHasScreen}}
        <computed-height data-type="document" data-recompute="{recomputeScreen}"
                        class="ase-overlay-screen" data-property="min-height"></computed-height>
    {{/if}}
    <div class="ase-overlay {{#if dataSheet}}ase-overlay-sheet{{else}}ase-overlay-popup{{/if}}"
         {{#if dataAriaLabelledby}}aria-labelledby={{dataAriaLabelledby}}{{/if}}
                 role="dialog" tabindex="-1">
        <content />
    </div>
</materializer>

</script>
	
	</body>
</html>