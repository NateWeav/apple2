    {{#if dataHasScreen}}
        <computed-height data-type="document" data-recompute="{recomputeScreen}"
                        class="ase-overlay-screen" data-property="min-height"></computed-height>
    {{/if}}
    <div class="ase-overlay {{#if dataSheet}}ase-overlay-sheet{{else}}ase-overlay-popup{{/if}}"
         {{#if dataAriaLabelledby}}aria-labelledby={{dataAriaLabelledby}}{{/if}}
                 role="dialog" tabindex="-1">
        <content />
    </div>
</materializer>

</script>
	
	</body>
</html>